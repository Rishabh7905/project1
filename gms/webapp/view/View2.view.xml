<mvc:View controllerName="gms.controller.View2"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Button id="_IDGenButton5" text="view2page"></Button>
    	 <VBox id="_IDGenVBox1" class="sapUiSmallMargin">
		<f:Form id="FormToolbar"
			editable="true"
			ariaLabelledBy="Title1">
			<f:toolbar>
				<Toolbar id="TB1">
					<Title id="Title1" text="Address"/>
					<ToolbarSpacer id="_IDGenToolbarSpacer1" />
					<Button id="_IDGenButton1" icon="sap-icon://settings"/>
					<Button id="_IDGenButton2" icon="sap-icon://drop-down-list" />
				</Toolbar>
			</f:toolbar>
			<f:layout>
				<f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1"
					labelSpanXL="4"
					labelSpanL="3"
					labelSpanM="4"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="4"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer id="_IDGenFormContainer1" ariaLabelledBy="Title2">
					<f:toolbar>
						<Toolbar id="_IDGenToolbar1">
							<Title id="Title2" text="Office"/>
							<ToolbarSpacer id="_IDGenToolbarSpacer2" />
							<Button id="_IDGenButton3" icon="sap-icon://settings"/>
						</Toolbar>
					</f:toolbar>
					<f:formElements>
						<f:FormElement id="_IDGenFormElement1" label="Name">
							<f:fields>
								<Input value="{SupplierName}" id="name"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement2" label="Street">
							<f:fields>
								<Input id="_IDGenInput1" value="{Street}" />
								<Input id="_IDGenInput2" value="{HouseNumber}">
									<layoutData>
										<l:GridData id="_IDGenGridData1" span="XL2 L1 M3 S4" />
									</layoutData>
								</Input>
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement3" label="ZIP Code/City">
							<f:fields>
								<Input id="_IDGenInput3" value="{ZIPCode}">
									<layoutData>
										<l:GridData id="_IDGenGridData2" span="XL2 L1 M3 S4" />
									</layoutData>
								</Input>
								<Input id="_IDGenInput4" value="{City}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement4" label="Country">
							<f:fields>
								<Select width="100%" id="country" selectedKey="{Country}">
									<items>
										<core:Item id="_IDGenItem1" text="Germany" key="Germany"/>
										<core:Item id="_IDGenItem2" text="USA" key="USA"/>
										<core:Item id="_IDGenItem3" text="England" key="England"/>
									</items>
								</Select>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
				<f:FormContainer id="_IDGenFormContainer2" ariaLabelledBy="Title3">
					<f:toolbar>
						<Toolbar id="_IDGenToolbar2">
							<Title id="Title3" text="Online"/>
							<ToolbarSpacer id="_IDGenToolbarSpacer3" />
							<Button id="_IDGenButton4" icon="sap-icon://settings"/>
						</Toolbar>
					</f:toolbar>
					<f:formElements>
						<f:FormElement id="_IDGenFormElement5" label="Web">
							<f:fields>
								<Input value="{Url}" type="Url" id="url"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement6" label="Twitter">
							<f:fields>
								<Input value="{Twitter}" id="twitter"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox> 
</mvc:View>